<div>
	<h1>Realtime Products</h1>
	{{#each products}}
		<div>
			<p>title: {{this.title}}</p>
	  		<p>description: {{this.description}}</p>
			<p>price: ${{this.price}}</p>
			<p>code: {{this.code}}</p>
			<p>id: {{this.id}}</p>
			<br>
		</div>
	{{/each}}
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>